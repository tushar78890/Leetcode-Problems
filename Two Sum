#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n ,val ;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
scanf("%d",&arr[i]);
    }
    scanf("%d",&val);
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
if(arr[i]+arr[j] == val){
printf("%d %d",i,j);
             }
        }
    }
    return 0;
}
